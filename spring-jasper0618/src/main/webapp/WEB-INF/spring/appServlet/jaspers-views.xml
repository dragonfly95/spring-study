<?xml version="1.0" encoding="UTF-8"?>
<beans:beans xmlns="http://www.springframework.org/schema/mvc"
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xmlns:beans="http://www.springframework.org/schema/beans"
             xmlns:context="http://www.springframework.org/schema/context"
             xsi:schemaLocation="http://www.springframework.org/schema/mvc http://www.springframework.org/schema/mvc/spring-mvc.xsd
		http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">

    <beans:bean id="firstPrint2"
                class="org.springframework.web.servlet.view.jasperreports.JasperReportsPdfView">
        <beans:property name="url" value="classpath:reports/first-print2.jrxml" />
        <beans:property name="reportDataKey" value="datasource" />
    </beans:bean>


    <beans:bean id="printLabel"
              class="org.springframework.web.servlet.view.jasperreports.JasperReportsPdfView">
     <beans:property name="url" value="classpath:reports/first-report.jrxml" />
     <beans:property name="reportDataKey" value="datasource" />
  </beans:bean>



    <beans:bean id="helloworld"
                class="org.springframework.web.servlet.view.jasperreports.JasperReportsPdfView">
        <beans:property name="url" value="classpath:reports/hello.jrxml" />
        <beans:property name="reportDataKey" value="datasource" />
    </beans:bean>

    <beans:bean id="blank"
                class="org.springframework.web.servlet.view.jasperreports.JasperReportsPdfView">
        <beans:property name="url" value="classpath:reports/blank.jrxml" />
        <beans:property name="reportDataKey" value="datasource" />
    </beans:bean>


</beans:beans>
